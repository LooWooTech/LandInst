<!DOCTYPE html>
<html>
<head>
    <title>@(ViewBag.Title) - 土地规划机构及从业人员信息管理平台</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="/content/bootstrap.css" media="screen">
    <link rel="stylesheet" href="/content/bootswatch.min.css">
    <link rel="stylesheet" href="/content/bootstrap-datepicker.css">
    <link rel="stylesheet" href="/content/main.css">
    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="/scripts/html5shiv.js"></script>
      <script src="/scripts/respond.min.js"></script>
    <![endif]-->
    <script src="/scripts/jquery-1.9.0.min.js"></script>
    <script src="/scripts/bootstrap.min.js"></script>
    <script src="/scripts/bootswatch.js"></script>
    <script src="/scripts/bootstrap-datepicker.js"></script>
    <script src="/scripts/common.js"></script>
</head>
<body>
    <div class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <a class="navbar-brand" href="/">土地规划机构及从业人员信息管理平台</a>
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-responsive-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

            </div>
            <div class="navbar-collapse collapse" id="navbar-main">
                @RenderSection("nav", false)
            </div>
        </div>
    </div>
    <div class="container">
        <br />
        <div class="panel panel-default" id="main-panel">
            <div class="panel-heading">
                <h3 class="panel-title">@(ViewBag.Title) </h3>
            </div>
            <div class="panel-body">
                @RenderBody()
            </div>
        </div>
        <footer>
            <div class="row">
                <div class="col-lg-12">
                    &copy;2014
                </div>
            </div>

        </footer>
    </div>
</body>
</html>
